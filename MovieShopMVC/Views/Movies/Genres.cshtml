@{
}
<h1>Here is paged Movie</h1>
@model ApplicationCore.Models.PagedResultSet<ApplicationCore.Models.MovieCardModel>

 <nav aria-label="Page navigation example">
    <ul class="pagination">
        @for (var i = 0; i < Model.TotalPages; i ++)
        {
            <li class="page-item">
                <a class="page-link" href="#">
                    @i
                </a>
            </li>
        }
    </ul>
</nav>